/* pages/guess/guess.wxss */
.guess{
  width: 100vw;
  height: 100vh;
  background: #c5e4c4;
  /* color: #333; */
  text-align: center;
  overflow: hidden;
}
.title{
  font-size: 40rpx;
  display: block;
  margin-top: 20rpx;
}
.victorynum{
  color: orangered;
}
.guessimg{
  width: 100%;
  height: 400rpx;
  /* background-color: white; */
  /* margin-top: 100rpx; */
  /* display: flex; */
}
.guessimg image{
  width: 200rpx;
  height: 200rpx;
  margin-top: 100rpx;
  vertical-align: middle;
}
.guessimg text{
  padding: 0 60rpx;
}
.vicmsg{
  margin-top: 50rpx;
  color: orangered;
  font-size: 40rpx;
}
.myguess{
  width: 90%;
  margin: 50rpx auto;
  height: 52%;
  background-color: white;
  border-radius: 10rpx;
  overflow: hidden;
}
.tip{
 margin-top: 50rpx;
}
.myquan{
  width: 100%;
  display: flex;
  margin: 50rpx 0;
}
.myquan view{
  flex: 1;
}
.myquan image{
  width: 180rpx;
  height: 180rpx;
  border-radius: 50%;
}
.btn{
  margin: 50rpx;
  background-color:#c5e4c4; 
  color: #333;
}